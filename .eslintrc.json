{
    "env": {
        "browser": true,
        "es6": true
    },
    // "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
"rules": {
  "prettier/prettier": "error"
},
"extends": [
  "prettier"],
"plugins": [
  "prettier"
]
}
